                    <div class="cus-register form-group" ng-controller="cusRegisterController">
                        <p class="cus-register-title">Đăng ký nhận tin</p>
                        <div class="form-group">
                            <input class="form-control" style="width: 80%; float: left" name="email" ng-keypress="($event.which === 13)?registe():0" ng-model="cusRegiste.email" id="email" placeholder="Email...">
                            <a class="btn" style="float: right; background-color: #337AB7; color: azure;"  ng-click="registe()">Đăng ký</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
